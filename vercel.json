{
  "source": ".",
  "functions": {
    "index.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "componentes/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "paginas/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/imagenes/(.*)",
      "dest": "/imagenes/$1"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/componentes/(.*)",
      "dest": "/componentes/$1"
    },
    {
      "src": "/paginas/(.*)",
      "dest": "/paginas/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}

