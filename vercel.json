{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/paginas/politica.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/imagenes/(.*)",
      "dest": "/imagenes/$1"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/componentes/(.*)",
      "dest": "/componentes/$1"
    },
    {
      "src": "/paginas/politica.php",
      "dest": "/api/paginas/politica.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}

